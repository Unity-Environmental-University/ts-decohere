{
  "name": "@decohere/ts-decohere",
  "version": "0.0.1-pre.1",
  "description": "TypeScript quantum-inspired type decoherence - synthesizing runtime values from complex type constraints using LLM-powered heuristic inference",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/decobase.ts",
      "default": "./src/decobase.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/ts-decohere.git"
  },
  "scripts": {
    "decohere": "ts-node tooling/decohere-build.ts",
    "play": "npm run decohere && ts-node src/playground.decohered.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "typescript",
    "type-synthesis",
    "llm",
    "constraint-solving",
    "test-generation"
  ],
  "author": "Unity Environmental University",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "openai": "^6.7.0"
  }
}
